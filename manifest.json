{  
  "manifest_version": 3,  
  "name": "CryptoPair Injector",  
  "version": "1.0",  
  "permissions": [  
    "activeTab",  
    "scripting"  
  ],  
  "content_scripts": [  
    {  
      "matches": ["*://*.baidu.com/*"],  
      "js": ["content_script.js"],  
      "run_at": "document_idle"  
    }  
  ],  
  "action": {  
    "default_popup": "popup.html",  
    "default_icon": {  
      "16": "images/icon16.png",  
      "48": "images/icon48.png",  
      "128": "images/icon128.png"  
    }  
  },  
  "icons": {  
    "16": "images/icon16.png",  
    "48": "images/icon48.png",  
    "128": "images/icon128.png"  
  }  
}